class Solution {


    public:
        int romanToInt(string s) {
                
                        map<char, int> a;
                                
                                        a.insert(pair<char,int>('I',1));
                                                a.insert(pair<char,int>('V',5));
                                                        a.insert(pair<char,int>('X',10));
                                                                a.insert(pair<char,int>('L',50));
                                                                        a.insert(pair<char,int>('C',100));
                                                                                a.insert(pair<char,int>('D',500));
                                                                                        a.insert(pair<char,int>('M',1000));
                                                                                                
                                                                                                        auto z = a.find(s[s.length()-1]);
                                                                                                                
                                                                                                                        int res = z->second;
                                                                                                                                
                                                                                                                                        
                                                                                                                                                cout<<res<<endl;
                                                                                                                                                        
                                                                                                                                                                // int res = a.get(s[s.length-1]);
                                                                                                                                                                        
                                                                                                                                                                                
                                                                                                                                                                                        for(int i = s.length()-2; i>=0;i--){
                                                                                                                                                                                                    auto j = a.find(s[i]);
                                                                                                                                                                                                                int an = j->second;
                                                                                                                                                                                                                            auto k = a.find(s[i+1]);
                                                                                                                                                                                                                                        int ann = k->second;
                                                                                                                                                                                                                                                    if(an>=ann){
                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                    res += a.find(s[i])->second;
                                                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                                                                                }
                                                                                                                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                                                                                                                        else{
                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                                                                                                                                                                         res -= a.find(s[i])->second;
                                                                                                                                                                                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                                                                                                                                                                                                     }
                                                                                                                                                                                                                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     }
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             return res;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         }
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         };ko


