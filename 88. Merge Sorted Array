class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int j = 0;

        for(int i = m; i <n+m;i++){
            // cout<<nums1[i]<<" : "<<nums2[i-m]<<endl;
            nums1[i] = nums2[i-m];

            
        }

        sort(nums1.begin(),nums1.end());
        
    }
};
