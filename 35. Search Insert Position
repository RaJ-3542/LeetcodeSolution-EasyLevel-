class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        
        int i = 0;
        
        
        while(nums[i]!=target ){
            
            if(nums[i]>target){
                
                break;
                
            }
            if(i==nums.size()-1){
                i++;
                break;
            }
            i++;
        }
        

        
        return i;
        
    }
};
